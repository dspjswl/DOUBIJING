<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>CheckUserInfoServlet</servlet-name>
    <servlet-class>com.hand.servlet.CheckUserInfoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleShowList</servlet-name>
  	<display-name>com.hand.servlet.HandleShowList</display-name>
  	<description></description>
  	<servlet-class>com.hand.servlet.HandleShowList</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleInsert</servlet-name>
  	<display-name>com.hand.servlet.HandleInsert</display-name>
  	<description></description>
  	<servlet-class>com.hand.servlet.HandleInsert</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleEdit</servlet-name>
  	<display-name>com.hand.servlet.HandleEdit</display-name>
  	<description></description>
  	<servlet-class>com.hand.servlet.HandleEdit</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleDelete</servlet-name>
  	<display-name>com.hand.servlet.HandleDelete</display-name>
  	<description></description>
  	<servlet-class>com.hand.servlet.HandleDelete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckUserInfoServlet</servlet-name>
    <url-pattern>/helpcheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleShowList</servlet-name>
  	<url-pattern>/com.hand.servlet.HandleShowList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleInsert</servlet-name>
  	<url-pattern>/com.hand.servlet.HandleInsert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleEdit</servlet-name>
  	<url-pattern>/com.hand.servlet.HandleEdit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleDelete</servlet-name>
  	<url-pattern>/com.hand.servlet.HandleDelete</url-pattern>
  </servlet-mapping>
</web-app>
